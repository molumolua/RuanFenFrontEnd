#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/crc-32/bin/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/crc-32/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules:/mnt/d/repositories/RuanFenFrontEnd/node_modules:/mnt/d/repositories/node_modules:/mnt/d/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/crc-32/bin/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/crc-32/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules:/mnt/d/repositories/RuanFenFrontEnd/node_modules:/mnt/d/repositories/node_modules:/mnt/d/node_modules:/mnt/d/repositories/RuanFenFrontEnd/mitech-v1.0.7/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../crc-32/bin/crc32.njs" "$@"
else
  exec node  "$basedir/../crc-32/bin/crc32.njs" "$@"
fi
